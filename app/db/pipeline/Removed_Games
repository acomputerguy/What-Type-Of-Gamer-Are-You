/**
 * To be run on: Unwinding_Yesterdays_Games
 */[
  {
    $project: {
      appids: "$appid"
    }
  },
  {
    $lookup: {
      from: "Unwinding_Todays_Games",
      localField: "appids",
      foreignField: "appid",
      as: "match"
    }
  },
  {
    $match: {
      match: {
        $size: 0
      }
    }
  },
  {
    $project: {
      appid: "$appids",
      _id: 0
    }
  },
  {
    $out: "Removed_Games"
  }
]